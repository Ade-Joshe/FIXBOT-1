{
  "hosting": {
    "public": "public/minisidebar",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
        "source": "/",
        "destination": "/minisidebar/index.html"
      },
      {
        "source": "/login",
        "destination": "/minisidebar/login.html.htm"
      },
      {
        "source": "/register",
        "destination": "/minisidebar/register.html.htm"
      }
    ]
  }
}
